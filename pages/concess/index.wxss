/* pages/concess/index.wxss */
/* 轮播 */  
.index-bg{width:100%;height:216rpx;position:relative;}
.index-bg swiper{width:100%;height:100%;position:relative;}
.index-bg .slide-navigator{width:100%;height:100%;}
.index-bg .slide-navigator .slide-image{width:100%;height:100%;}
.index-bg .wx-swiper-dot{width:15rpx;height:15rpx;border-radius:50%;}
.index-bg .wx-swiper-dots.wx-swiper-dots-horizontal{margin-bottom:5rpx;}
/* 列表 */
.concess-list{margin: 0 30rpx 80rpx 30rpx;}
.concess-list .item{margin-top: 30rpx;}
.concess-list .item .item-top{padding: 20rpx 30rpx 0 30rpx;}
.concess-list .item .item-top .distance{color:#999;}
.concess-list .item .line{width: 100%;height: 49rpx;}
.concess-list .item .line image{width: 100%;height: 100%;}
.concess-list .item .item-bottom{padding: 0 30rpx 20rpx 30rpx;}
.concess-list .item .item-bottom .item-bottom-l .pictrue{width: 150rpx;height: 150rpx;display: block;}
.concess-list .item .item-bottom .item-bottom-l .pictrue image{width: 100%;height:100%;border-radius: 15rpx;}
.concess-list .item .item-bottom .item-bottom-l .item-bottom-l-text{margin-left: 20rpx;}
.concess-list .item .item-bottom .item-bottom-l .item-bottom-l-text .sub{color:#CE4E63;font-size: 22rpx;margin-top: 10rpx;}
.concess-list .item .item-bottom .item-bottom-l .item-bottom-l-text .progress{background:#EFEDEE;overflow: hidden;width:260rpx;border-radius:30rpx;height:15rpx;position:relative;margin:5rpx 0 20rpx 0;}
.concess-list .item .item-bottom .item-bottom-l .item-bottom-l-text .progress .bg-red{width: 0;height: 100%;transition: width 0.6s ease;background-color: #CE4E63;border-radius: 30rpx;}
.concess-list .item .item-bottom .item-bottom-l .item-bottom-l-text .vip{padding:5rpx 20rpx;background-image: url('data:image/png;base64,');background-size:100% 100%;width:240rpx;}
.concess-list .item .item-bottom .item-bottom-l .item-bottom-l-text .vip view:first-child{color:#5C4602}
.concess-list .item .item-bottom .item-bottom-l .item-bottom-l-text .vip view:last-child{color:#fff;margin-right: 15rpx;}
.concess-list .item .item-bottom .item-bottom-r{background:#CE4E63;color:#fff;border-radius: 50rpx;padding:15rpx 30rpx;}
/* 弹窗 */
.popup{background:#fff;width: 550rpx;overflow: hidden; position: fixed;top: 50%;z-index: 1000;left: 0;margin: -150px 100rpx 0 100rpx;text-align: center;padding: 50rpx 40rpx 40rpx 40rpx;border-radius: 15rpx;}
.popup .title{color:#CE4E63;font-size: 32rpx;}
.popup .title2{color:#BFA553;font-size: 32rpx;}
.popup .title3{color:#BFA553;font-size: 22rpx;margin-top: 15rpx;}
.popup .coupon-list{margin: 50rpx 0 30rpx 0;}
.popup .coupon-list .item{border: 2rpx solid #CE4E63;border-radius: 15rpx;padding: 10rpx 20rpx;font-size: 22rpx;line-height: 1.5;color:#CE4E63}
.popup .coupon-list .item .price{font-size: 28rpx;}
.popup .coupon-list .item .price text{font-size: 34rpx;}
.popup .coupon-list .item.active{color:#BFA553;}
.popup .btn{margin:0 50rpx;}
.popup .btn view{background:#CE4E63;color:#fff;padding: 20rpx 0;text-align: center;border-radius: 10rpx;}