/* pages/shopDetails/index.wxss */
page{background:#F8F6F7;}
/* 顶部 */
.header .header-bg{height: 200rpx;background:#CE4E63;}
.header .header-item{margin-top: -170rpx;padding: 0 80rpx;}
.header .header-item .item-l,.header .header-item .item-r{width:80rpx;height: 80rpx;margin-bottom: 30rpx;}
.header .header-item .item-m{width:240rpx;height: 240rpx;}
.header .header-item image{width:100%;height: 100%;}
.header .header-text{text-align: center;line-height: 1.5;margin: 30rpx 20rpx;}
.header .header-text view:last-child{margin-top: 15rpx;}
/* 切换 */
.nav{background:#fff;margin: 0 30rpx;padding:30rpx 20rpx;border-radius: 15rpx;}
.nav .item{margin-right: 50rpx;}
.nav .item.on{color:#CE4E63;font-weight: bold;}
/* 优惠 */
.discount{margin:20rpx 30rpx 80rpx 30rpx;}
.discount .one-item{padding: 0 20rpx 20rpx 20rpx;border-bottom: 2rpx solid #ccc;}
.discount .one-item .item{padding-top: 30rpx;}
.discount .item .title image{width:30rpx;height: 30rpx;margin-right: 20rpx;}
.discount .one-item .item .content{background-image: url('data:image/png;base64,');background-size: 100% 100%;padding: 30rpx;margin-top: 8rpx;}
.discount .one-item .item .content .content-l .price{color:#D25669}
.discount .one-item .item .content .content-r{background:#CE4E63;color:#fff;padding:5rpx 15rpx;border-radius: 5rpx;}
.discount .two-item{padding: 20rpx;border-bottom: 2rpx solid #ccc;}
.discount .two-item .content{padding-top: 20rpx;}
.discount .two-item .content .content-l .pictrue{width: 150rpx;height: 150rpx;}
.discount .two-item .content .content-l .pictrue image{width: 100%;height: 100%;}
.discount .two-item .content .content-r{width: 480rpx;margin-left: 20rpx;border-bottom:2rpx solid #ccc;padding-bottom: 20rpx;}
.discount .two-item .content:last-child .content-r{border-bottom:none;}
.discount .two-item .content .content-r .text{width: 360rpx;}
.discount .two-item .content .content-r .price{color:#CE4E63;}
.discount .two-item .content .content-r .line-price{margin-left: 20rpx;}
.discount .two-item .content .content-r .btn{background: #CE4E63;color: #fff;padding: 5rpx 15rpx;border-radius: 5rpx;margin-right: 30rpx;}
.discount .three-item{padding: 20rpx 20rpx 60rpx 20rpx;}
.discount .three-item .content{padding-top: 20rpx;}
.discount .three-item .content .content-l .pictrue{width: 150rpx;height: 150rpx;}
.discount .three-item .content .content-l .pictrue image{width: 100%;height: 100%;}
.discount .three-item .content .content-r{width: 480rpx;margin-left: 20rpx;padding-bottom: 20rpx;}
.discount .three-item .content .content-r .text{width: 360rpx;}
.discount .three-item .content .content-r .price{color:#CE4E63;}
.discount .three-item .content .content-r .line-price{margin-left: 20rpx;}
.discount .three-item .content .content-r .btn{background: #CE4E63;color: #fff;padding: 5rpx 15rpx;border-radius: 5rpx;margin-right: 30rpx;}
/* 评价 */
.evaluate{padding: 0 30rpx;margin: 20rpx 30rpx;border-radius: 15rpx;}
.evaluate .item{padding: 30rpx 0;border-bottom: 2rpx dashed #ccc;}
.evaluate .item:last-child{border-bottom:none;}
.evaluate .item .pictrue{width: 80rpx;height: 80rpx;}
.evaluate .item .pictrue image{width: 100%;height: 100%}
.evaluate .item .text{width:520rpx;margin-left: 30rpx;}
.evaluate .item .text .text-top{margin-bottom: 30rpx;}
.evaluate .item .text .img-item{width: 200rpx;height: 200rpx;margin: 30rpx 0;}
.evaluate .item .text .img-item image{width: 100%;height: 100%;}
.evaluate .item .text .news{background:#F8F6F7;padding: 30rpx 30rpx 0rpx 30rpx;position: relative;margin-top: 50rpx;}
.evaluate .item .text .news .news-item{padding-bottom: 30rpx;}
.evaluate .item .text .news .news-item .title{padding-bottom:30rpx;}
.evaluate .item .text .news .triangle{position: absolute;top:-15rpx;left:40rpx;width: 40rpx;height: 40rpx;background:#F8F6F7;transform: rotate(45deg)}
/* 商家 */
.store .album,.store .service,.store .qualifications{margin: 20rpx 30rpx 0 30rpx;padding: 20rpx;border-radius: 15rpx;}
.store .album .scroll-album{margin-top: 20rpx;}
.store .album .scroll-album .image-group{display: flex;white-space: nowrap;}
.store .album .scroll-album .item .img-box{width: 100%;height: 150rpx;}
.store .album .scroll-album .image-group image{width: 180rpx;height: 180rpx;margin-right: 20rpx;}
.store .message{line-height: 3;margin: 20rpx 30rpx 0 30rpx;padding:0 20rpx;border-radius: 15rpx;}
.store .message view{border-bottom: 2rpx solid #ccc;}
.store .message view:last-child{border-bottom: none;}
.store .message view .icon{width: 50rpx;height: 50rpx;}
.store .service .title{margin-bottom: 20rpx;}
.store .qualifications .icon-xiala{transform: rotate(-90deg)}
.store .code {text-align: center;margin-bottom: 40rpx;}
.store .code image{width: 300rpx;height: 300rpx;margin: 30rpx 0;}
